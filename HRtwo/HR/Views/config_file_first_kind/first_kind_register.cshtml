
@{
    Layout = null;
}

<!DOCTYPE html>
@model Model.config_file_first_kindModel
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>first_kind_register</title>
    <link href="~/css/table.css" rel="stylesheet" />
    <script src="~/javascript/comm/comm.js"></script>
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <script src="~/Scripts/jquery.validate.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.js"></script>
    <script type="text/javascript">
        function show(msg) {
            if (msg == "ok") {
                alert("新增成功!!!")
                window.location.href = "/config_file_first_kind/first_kind";
            }
        }
    </script>
</head>
<body>
@using (Ajax.BeginForm("first_kind_register", new AjaxOptions() { HttpMethod = "Post", OnSuccess = "show" }))
{
    <table width="100%">
        <tr>
            <td>
                <font color="black">
                    您正在做的业务是：人力资源--客户化设置--人力资源档案管理设置--I级机构设置--I级机构添加
                </font>
            </td>
        </tr>
        <tr>
            <td align="right">
                <input type="submit" value="提交" class="BUTTON_STYLE1">
                <input type="button" value="返回" class="BUTTON_STYLE1"
                       onclick="history.back();">
            </td>
        </tr>
    </table>

        <table width="100%" border="1" cellpadding=0 cellspacing=1
               bordercolorlight=#848284 bordercolordark=#eeeeee
               class="TABLE_STYLE1">
            <tr>
                <td width="19%" class="TD_STYLE1">
                    I级机构编号
                </td>
                <td class="TD_STYLE2">@Html.TextBox("first_kind_id", Model.first_kind_id, new { @class = "INPUT_STYLE1", @readonly = "readonly" })</td>
               
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    I级机构名称
                </td>
                <td class="TD_STYLE2">
                    @*<input type="text" name="cffk.firstKindName" value=""
                           class="INPUT_STYLE1">*@
                    @Html.TextBox("first_kind_name", Model.first_kind_name, new { @class = "INPUT_STYLE1"})
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    薪酬发放责任人编号（多个编号之间请用"半角逗号"加"一个空格"隔开，如", "）
                </td>
                <td class="TD_STYLE2">
                    @Html.TextArea("first_kind_salary_id", Model.first_kind_salary_id, new { @class = "TEXTAREA_STYLE1", @rows = 4 });
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    销售责任人编号（多个编号之间请用"半角逗号"加"一个空格"隔开，如", "）
                </td>
                <td class="TD_STYLE2">
                    @Html.TextArea("first_kind_sale_id", Model.first_kind_sale_id, new { @class = "TEXTAREA_STYLE1", @rows = 4 });
                </td>
            </tr>
        </table>
}
  
</body>
</html>
